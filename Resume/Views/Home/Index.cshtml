@model BigOne
@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Super CV</h1>
</div>


<h1>Education</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Education.SchoolName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Education.QualifiedYear)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (Education item in Model.Educations)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.SchoolName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.QualifiedYear)
                </td>
            </tr>
        }
    </tbody>
</table>

